<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" version="3.1">
  <display-name>TVS</display-name>
  <welcome-file-list>
    <welcome-file>Login.jsp</welcome-file>
  </welcome-file-list>
 <context-param>
    <param-name>ab-ab</param-name>
    <param-value>/WEB-INF/log4j.properties</param-value>
  </context-param>
  <listener>
  <listener-class>com.tvs.version.AppListner</listener-class>
  </listener>
  <servlet>
    <servlet-name>Welcome</servlet-name>
    <jsp-file>/WEB-INF/Welcome.jsp</jsp-file>
  </servlet>
  <servlet-mapping>
    <servlet-name>Welcome</servlet-name>
    <url-pattern>/ui</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>qqq</servlet-name>
    <jsp-file>/ChangePassword2.jsp</jsp-file>
  </servlet>
  <servlet-mapping>
    <servlet-name>qqq</servlet-name>
    <url-pattern>/secure</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>abc</servlet-name>
    <servlet-class>com.tvs.login.LoginServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>abc</servlet-name>
    <url-pattern>/Login</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>abcd</servlet-name>
    <servlet-class>Sample</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>abcd</servlet-name>
    <url-pattern>/sample</url-pattern>
  </servlet-mapping>
 
  <servlet>
    <servlet-name>abd</servlet-name>
    <servlet-class>Talendcode</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>abd</servlet-name>
    <url-pattern>/talendcode</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>abbd</servlet-name>
    <servlet-class>CustomerCode</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>abbd</servlet-name>
    <url-pattern>/CustomerCode</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>abbbd</servlet-name>
    <servlet-class>VendorCode</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>abbbd</servlet-name>
    <url-pattern>/VendorCode</url-pattern>
  </servlet-mapping>
  <resource-ref>
    <description>postgreSQL Datasource example</description>
    <res-ref-name>MDM/Config</res-ref-name>
    <res-type>javax.sql.DataSource</res-type>
    <res-auth>Container</res-auth>
  </resource-ref>
  <resource-ref>
    <description>postgreSQL Datasource example</description>
    <res-ref-name>MDM/ETL</res-ref-name>
    <res-type>javax.sql.DataSource</res-type>
    <res-auth>Container</res-auth>
  </resource-ref>
  <session-config>
    <session-timeout>15</session-timeout>
  </session-config>
  <error-page>
    <error-code>405</error-code>
    <location>/Login.jsp</location>
  </error-page>
  <servlet>
    <servlet-name>ErrorHandler</servlet-name>
    <servlet-class>Errorhandler</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ErrorHandler</servlet-name>
    <url-pattern>/Errorhandler</url-pattern>
  </servlet-mapping>
   <error-page>
    <error-code>500</error-code>
    <location>/Errorhandler</location>
  </error-page> 
  
</web-app>